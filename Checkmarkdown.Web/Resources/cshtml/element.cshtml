@using Checkmarkdown.Core.Elements;
@using Checkmarkdown.Core.Elements.Meta;
@inherits RazorLight.TemplatePage<Element>;
@model Element;
@switch (Model) {
    case Heading:
        await IncludeAsync("elements/heading.cshtml", Model);
        break;
    case Item:
        await IncludeAsync("elements/item.cshtml", Model);
        break;
    case Listing:
        await IncludeAsync("elements/listing.cshtml", Model);
        break;
    case ListItem:
        await IncludeAsync("elements/list-item.cshtml", Model);
        break;
    case Paragraph:
        await IncludeAsync("elements/paragraph.cshtml", Model);
        break;
    default:
        <span>Unrecognized element type: < @Model.GetType().Name ></span>
        break;
}