@inherits RazorLight.TemplatePage<Listing>;
@using Checkmarkdown.Core.Elements;
@using Checkmarkdown.Web

@model Listing;

@{ var tagName = Model.Kind == ListingKind.Ordered ? "ol" : "ul"; }

<@tagName @Model.HtmlAttributes()>
    @{ await IncludeAsync("elements.cshtml", Model?.Children); }
</@tagName>
